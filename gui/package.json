{
	"name": "gui",
	"version": "0.0.0",
	"license": "MIT",
	"private": true,
	"main": "src/server/index.js",
	"scripts": {
		"orig-start-local": "webpack-dev-server --env.local --progress --hot --open",
		"orig-start": "webpack-dev-server --progress --hot --open",
		"build": "webpack --mode production",
		"start": "npm run build && node src/server/index.js",
		"client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
		"server": "nodemon src/server/index.js",
		"dev": "concurrently \"npm run server\" \"npm run orig-start\""
	},
	"dependencies": {
		"@deck.gl/experimental-layers": "^6.4.0",
		"@deck.gl/mapbox": "^6.4.0",
		"deck.gl": "^6.4.0",
		"react": "^16.3.0",
		"react-dom": "^16.3.0",
		"react-map-gl": "^3.3.0",
		"express-ws": "^4.0.0"
	},
	"devDependencies": {
		"buble": "^0.19.3",
		"buble-loader": "^0.5.0",
		"clean-webpack-plugin": "^1.0.0",
		"concurrently": "^4.0.0",
		"html-webpack-plugin": "^3.2.0",
		"http-proxy-middleware": "^0.19.1",
		"nodemon": "^1.17.3",
		"webpack": "^4.20.2",
		"webpack-cli": "^3.1.2",
		"webpack-dev-server": "^3.1.1"
	}
}
